<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:simpleType name="standAloneNameType">
    <xs:annotation>
      <xs:documentation xml:lang="DE">
            Roboter-Achsnamen
         </xs:documentation>
      <xs:documentation xml:lang="EN">
            robot axis name
         </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:normalizedString">
      <xs:pattern value="(A|E)[1-6]"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="StandAlone">
    <xs:complexType>
      <xs:attribute name="Name" type="standAloneNameType" use="required"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="Axis">
    <xs:complexType>
         <xs:sequence>
            <xs:element ref="StandAlone"/>
         </xs:sequence>
      <xs:attribute name="ID" type="axisIDType" use="required"/>
      <xs:attribute name="Mode" type="axisModeType" use="optional" default="StandAlone"/>
      <xs:attribute name="ControlType" type="axisControlTypeType" use="optional" default="PositionTorqueControl"/>
      <xs:attribute name="Coupling" type="xs:boolean" default="false"/>
    </xs:complexType>
  </xs:element>
</xs:schema>
